@import 'nib'

#jmpress
  width 100%
  height 600px
  display inline-block

  & h2
    font 120px HelveticaNeue, Helvetica, Ariel, Sans-serif
    font-weight 100

  & .mainpage
    position relative

    & span
      font-size 50px

    & .one
      position absolute
      top 0px
      left 0px

    & .two
      position absolute
      top 300px
      left 400px

    & .three
      absolute top 0px left 400px

    & .four
      absolute top -300px left 400px

    & .five
      absolute top -300px right 400px

    & .six
      absolute top 0px right 400px

    & .seven
      absolute top 300px right 400px

.ui-widget-content
  border 1px solid #aaa
  background #eee
  color #222